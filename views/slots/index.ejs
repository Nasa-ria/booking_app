<div>
    <div >
        <ul> 
           
    <% if (typeof slots != 'undefined'){  %> 
        <%	slots.forEach((slot) =>{ %>
            <li> 
    <p>slot date : <%- slot.slot_date.getDate() %>-0<%- slot.slot_date.getMonth() %>-<%- slot.slot_date.getFullYear() %></p>
